{
  "manifest_version": 2,
  "name": "Insta100x Redirect",
  "version": "1.0",
  "description": "Redirects Instagram to https://www.youtube.com/watch?v=LLIwKiQ362E",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.instagram.com/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
