{
  "manifest_version": 2,
  "name": "Netflix Redirect",
  "version": "1.0",
  "description": "Redirects Netlfix to https://www.youtube.com/watch?v=LLIwKiQ362E",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*:https://www.netflix.com/in/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
